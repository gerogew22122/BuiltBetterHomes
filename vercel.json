{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/",
      "destination": "/wordpress-index.html"
    },
    {
      "source": "/contact",
      "destination": "/index.html"
    },
    {
      "source": "/:path((?!api|wp-content|wp-includes).*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/api/contact",
      "dest": "/api/contact.ts"
    }
  ]
}
